<!doctype html>
<html<%- typeof(htmlAttributes) !== 'undefined' && htmlAttributes ? htmlAttributes : '' %>>
  <head>
    <%- typeof(htmlHead) !== 'undefined' && htmlHead ? htmlHead : '' %>
    <%- typeof(renderedCss) !== 'undefined' && renderedCss ? renderedCss : '' %>
  </head>
  <body>
    <div id="app">
      <%- typeof(renderedContent) !== 'undefined' && renderedContent ? renderedContent : '' %>
    </div>
    <% if (typeof(initialState) !== 'undefined' && initialState) { %>
      <script>window.__INITIAL_STATE__ = <%- initialState %>;</script>
    <% } %>
    <% if (typeof(apiUri) !== 'undefined' && apiUri) { %>
      <script>window.__API__ = <%- apiUri %>;</script>
    <% } %>
  </body>
</html>
